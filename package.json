{
  "name": "documentation",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "prestart": "npm run gen-api-docs",
    "build": "docusaurus build",
    "prebuild": "npm run gen-api-docs",
    "swizzle": "docusaurus swizzle",
    "clear": "docusaurus clear",
    "serve": "node serve.js",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "gen-api-docs": "docusaurus gen-api-docs all --plugin-id theoads-api && docusaurus gen-api-docs all --plugin-id millicast-api && docusaurus gen-api-docs all --plugin-id theolive-api",
    "clean-api-docs": "docusaurus clean-api-docs all --plugin-id theoads-api && docusaurus clean-api-docs all --plugin-id millicast-api && docusaurus clean-api-docs all --plugin-id theolive-api",
    "typecheck": "tsc",
    "check-format": "prettier --check .",
    "format": "prettier --write .",
    "lint": "eslint docusaurus.config.ts \"sidebars*.ts\" src",
    "fix-lint": "npm run lint -- --fix"
  },
  "dependencies": {
    "@docusaurus/core": "^3.8.0",
    "@docusaurus/faster": "^3.8.0",
    "@docusaurus/plugin-client-redirects": "^3.8.0",
    "@docusaurus/plugin-google-gtag": "^3.8.0",
    "@docusaurus/plugin-google-tag-manager": "^3.8.0",
    "@docusaurus/preset-classic": "^3.8.0",
    "@mdx-js/react": "^3.1.0",
    "@webcoder49/code-input": "^2.2.1",
    "clsx": "^2.1.1",
    "docusaurus-plugin-image-zoom": "^3.0.1",
    "docusaurus-plugin-openapi-docs": "^4.4.0",
    "docusaurus-theme-openapi-docs": "^4.4.0",
    "prism-react-renderer": "^2.4.1",
    "prismjs": "^1.30.0",
    "react": "^19.0.0",
    "react-aria-components": "^1.9.0",
    "react-dom": "^19.1.0",
    "schema-dts": "^1.1.2",
    "url": "^0.11.4"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.8.0",
    "@docusaurus/tsconfig": "^3.8.0",
    "@docusaurus/types": "^3.8.0",
    "@eslint/js": "^9.28.0",
    "dotenv": "^16.5.0",
    "eslint": "^9.28.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^6.0.0-rc.1",
    "exports-loader": "^5.0.0",
    "globals": "^16.2.0",
    "imports-loader": "^5.0.0",
    "lodash": "^4.17.21",
    "prettier": "^3.5.3",
    "raw-loader": "^4.0.2",
    "remark": "^15.0.1",
    "serve-handler": "^6.1.6",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.0",
    "unist-util-visit": "^5.0.0"
  },
  "overrides": {
    "serve-handler": {
      "path-to-regexp": "^3.3.0"
    }
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}
